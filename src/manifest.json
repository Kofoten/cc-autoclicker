{
  "manifest_version": 3,
  "name": "Auto Clicker for Cookie Clicker",
  "version": "1.1",
  "description": "Automatically clicks cookies and shimmers.",
  "host_permissions": [
    "*://orteil.dashnet.org/*"
  ],
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://orteil.dashnet.org/cookieclicker",
        "*://orteil.dashnet.org/cookieclicker/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "injected_cheat.js"
      ],
      "matches": [
        "*://orteil.dashnet.org/*"
      ]
    }
  ]
}